type User {
    user_id : Int!,
    user_name: String!,
    user_password: String!,
    user_email: String
}

type Query {
    getUserById(user_id:Int):User,
    getUsers: [User],
}

type Mutation{
    createUser(user_name:String!, user_password:String!, user_email:String!):User,
    updateUser(user_id:Int!, user_email:String!):User,
    deleteUser(user_id:Int!):User
}

